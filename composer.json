        "symfony/filesystem": ">=2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Djordje\\Filebrowser": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
