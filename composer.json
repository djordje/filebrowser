{
    "name": "djordje/filebrowser",
    "description": "File browser for PHP",
    "keywords": ["file", "browser", "php", "storage", "explorer"],
    "license": "MIT",
    "homepage": "https://github.com/djordje/filebrowser",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Djordje Kovacevic",
            "homepage": "http://djordjekovacevic.com"
        }
